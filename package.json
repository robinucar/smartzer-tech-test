    "concurrently": "^9.1.2",
    "cypress": "^14.5.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-import": "2.31.0",
