    "@nx/eslint": "20.8.2",
    "@nx/eslint-plugin": "20.8.2",
    "@nx/express": "^20.8.2",
    "@nx/jest": "20.8.2",
    "@nx/js": "20.8.2",
    "@nx/node": "20.8.2",
    "@nx/react": "^20.8.2",
    "@nx/vite": "20.8.2",
    "@nx/web": "20.8.2",
    "@nx/webpack": "21.2.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.0",
    "@svgr/webpack": "^8.1.0",
    "@swc-node/register": "~1.10.10",
    "@swc/cli": "~0.7.7",
    "@swc/core": "~1.12.3",
    "@swc/helpers": "~0.5.17",
    "@swc/jest": "~0.2.38",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.32",
    "@types/react": "19.1.8",
